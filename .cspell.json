  "ignoreWords": [
    "README",
    "TODO",
    "FIXME",
    "HACK",
    "XXX",
    "RUSTSEC",
    "widestring",
    "sddl",
    "libc"
  ],
  "patterns": [
    {
      "name": "Rust code",
      "pattern": "/```rust[\\s\\S]*?```/g"
    },
    {
      "name": "Code blocks", 
      "pattern": "/`[^`]+`/g"
    },
    {
      "name": "URLs",
      "pattern": "/https?:\\/\\/[^\\s]+/g"
    },
    {
      "name": "File paths",
      "pattern": "/[.\\w\\/\\\\-]+\\.[a-z]{2,4}/gi"
    }
  ],
  "ignoreRegExpList": [
    "/\\b[A-Z]{2,}\\b/g",
    "/\\b\\w*[0-9]\\w*/g",
    "/\\b[a-f0-9]{8,}\\b/gi"
  ],
  "files": [
    "**/*.md",
    "**/*.rs"
  ],
  "ignorePaths": [
    "target/**",
    ".git/**",
    "node_modules/**",
    "*.lock"
  ]
}
